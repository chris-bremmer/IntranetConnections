{
  "title": "Feedback Form",
  "fields": [
    { "type": "text", "label": "Name", "required": true },
    { "type": "email", "label": "Email", "required": true },
    { "type": "rating", "label": "Overall Experience", "min": 1, "max": 10, "required": true },
    { "type": "textarea", "label": "Comments", "required": false },
    { "type": "checkbox", "label": "Would you recommend us?", "required": false },
    { "type": "dropdown", "label": "Platform Used", "values": ["Web", "Mobile", "Both"], "required": true },
    { "type": "text", "label": "Suggestions for Improvement", "required": false },
    { "type": "date", "label": "Date of Visit", "required": false }
  ]
}