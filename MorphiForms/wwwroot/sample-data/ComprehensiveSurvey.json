{
  "title": "Comprehensive Survey",
  "fields": [
    { "type": "text", "label": "First Name", "required": true },
    { "type": "text", "label": "Last Name", "required": true },
    { "type": "email", "label": "Email Address", "required": true },
    { "type": "number", "label": "Age", "min": 18, "max": 100, "required": true },
    { "type": "dropdown", "label": "Education Level", "values": ["High School", "Bachelor's", "Master's", "Doctorate"], "required": true },
    { "type": "radio", "label": "Employment Status", "values": ["Employed", "Unemployed", "Student"], "required": true },
    { "type": "textarea", "label": "Hobbies", "required": false },
    { "type": "number", "label": "Household Income ($)", "required": true },
    { "type": "checkbox", "label": "Agree to Share Data", "required": true },
    { "type": "dropdown", "label": "Preferred Contact Method", "values": ["Email", "Phone", "Mail"], "required": true },
    { "type": "number", "label": "Satisfaction Level", "min": 1, "max": 10, "required": true },
    { "type": "text", "label": "Favorite Brand", "required": false },
    { "type": "checkbox", "label": "Subscribed to Newsletter", "required": false },
    { "type": "date", "label": "Subscription Date", "required": false },
    { "type": "text", "label": "City of Residence", "required": false },
    { "type": "textarea", "label": "Additional Feedback", "required": false },
    { "type": "number", "label": "Years at Current Address", "required": false },
    { "type": "dropdown", "label": "Access to Technology", "values": ["Smartphone", "Computer", "Tablet"], "required": false },
    { "type": "number", "label": "Tech Savviness", "min": 1, "max": 5, "required": false }
  ]
}